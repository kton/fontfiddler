{
  "name": "Font Fiddler",
  "description": "A React-based extension to manage local fonts",
  "version": "0.0.2",
  "manifest_version": 2,
  "icons": {"128": "icon.png"},
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Font Fiddler",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "fontSettings"
  ]
}
